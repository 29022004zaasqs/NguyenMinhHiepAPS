@model IEnumerable<NMHBaiThiGiuaKy.Models.NmhProduct>

@{
    ViewBag.Title = "NMH";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Them moi", "Create")
</p>
<table class="table table-bordered">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NMHName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NMHImage)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NMHQuantity)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NMHPrice)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NMHIsActive)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NMHName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NMHImage)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NMHQuantity)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NMHPrice)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NMHIsActive)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
